{% extends "base.html.twig" %}

{% block title %}Forum - Modifier mot de passe{% endblock %}

{% block body %}
<main>
    <h1>Modifier votre mot de passe</h1>

    {% for message in app.flashes('success') %}
        <div>
            {{message}}
        </div>
    {% endfor %}
    {% for message in app.flashes('error') %}
        <div>
            {{message}}
        </div>
    {% endfor %}

    {{ form(form) }}
</main>
{% endblock %}